{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (MyConsoleApp)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build (MyConsoleApp)",
      "program": "${workspaceFolder}/src/MyConsoleApp/bin/Debug/netcoreapp3.1/MyConsoleApp.dll",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
